<Window x:Class="TestProject.KernelWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TestProject"
		  xmlns:nf="clr-namespace:NinjectFrame;assembly=NinjectFrame"
        mc:Ignorable="d"
		  Loaded="Window_Loaded"
        Title="{Binding ElementName=Frame, Path=Content.Title}" Height="450" Width="800">
	<Grid>
		<nf:KernelFrame Name="Frame" NavigationUIVisibility="Visible"></nf:KernelFrame>
	</Grid>
</Window>
